{{#if posts.length}}
<section class="section">
  <h2>Your Posts</h2>
    {{#each posts as |post|}}
      {{> dashboard-postinfo post}}
    {{/each}}
</section>
{{/if}}
<section class="section">
  <div class="container">
    <div class="level">
      <div class="level-item">
        <a class="button is-info is-large" href="/dashboard/create">Create Post</a>
      </div>
    </div>
  </div>
</section>

<script src="/javascript/add-post.js"></script>