<div class="container">
  <form class="sign-up-form">
    <div class="columns box m-2">
      <div class="column is-one-third field">
        <h1 class="title">Sign Up</h1>
        <div class="field">
          <label class="label">Username</label>
          <input id="username-signup" class="input" type="text" placeholder="username">
        </div>
        <div class="field">
          <label class="label">Password</label>
          <input id="password-signup" class="input" type="current-password" placeholder="password">
        </div>
        <div class="field is-grouped">
            <button type="submit" class="button is-info">Create Account</button>
        </div>
      </div>
  </form>
</div>
<script src="/javascript/signup.js"></script>


